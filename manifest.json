{
    "name": "Bing Wallpaper for Chromebookâ„¢",
    "short_name": "Bing Wallpaper",
    "version": "0.1.9",
    "description": "Automatically sets Bing's image of the day as the desktop wallpaper on Chromebooks and other ChromeOS devices.",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "64": "icons/64.png",
        "128": "icons/128.png"
    },
    "permissions": ["alarms", "downloads", "notifications", "storage", "wallpaper", "https://www.bing.com/"],
    "browser_action"  :  {
        "default_title": "Bing Wallpaper for Chromebook",
        "default_icon"   : "icons/32.png", 
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "manifest_version": 2,
    "author": "chew-z"
}
